<name>sp_widget_0a6766c62fccd850d5b2d3f62799b605</name>
<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sp_widget"&gt;&lt;sp_widget action="INSERT_OR_UPDATE"&gt;&lt;category&gt;custom&lt;/category&gt;&lt;client_script&gt;&lt;![CDATA[function($scope, spUtil) {
    /* widget controller */
    var c = this;

	//Watch function to set the focus on the form field and trigger when it changes
    $scope.$watch(function() {
            return ($scope.page.g_form.getValue('date_and_time'));
        },
        $scope.runThisFunction = function(value) {
            c.data.date_and_time = value;
		//updating the server with new values
            c.server.update().then(function(response) {
                spUtil.update($scope);

				//List of variables being used to do the calculations
                $scope.calVal = [];
                $scope.striker = [];
                $scope.currentMonth = $scope.data.currentMonth;
                $scope.currentYear = $scope.data.currentYear;
                $scope.currentDay = $scope.data.currentDay;
                $scope.currentDate = $scope.data.currentDate;
				$scope.today = $scope.data.today;
				$scope.strikerValueDate = $scope.data.strikerValueDate;
				$scope.strikerValueDay = $scope.data.strikerValueDay;
				$scope.strikerValueMonth = $scope.data.strikerValueMonth;
				$scope.strikerValueYear = $scope.data.strikerValueYear;
				$scope.reservedAndBlockedDates = $scope.data.reservedAndBlockedDates;
                $scope.text = "These are the available dates for " + $scope.currentMonth + " " + $scope.currentYear;

                var monthStart = $scope.data.monthStartDay;
                var numbering = 1;
				//loop to set the dates on the calendar
                for (var i = 1; i &lt; 43; i++) {
                    $scope.striker[i] = false;
					
					if (numbering &lt; $scope.today &amp;&amp; $scope.strikerValueMonth == 0 &amp;&amp; $scope.strikerValueYear == 0) {
                        $scope.striker[i] = true;
                    }
                    if (i &lt; 7) {
                        if (monthStart == i) {
                            $scope.calVal[i] = numbering;
							if($scope.reservedAndBlockedDates.indexOf(numbering)&gt;-1){
								$scope.striker[i] = true;
							}
                            numbering++;
                        } else if (monthStart &lt; i) {
                            $scope.calVal[i] = numbering;
							if($scope.reservedAndBlockedDates.indexOf(numbering)&gt;-1){
								$scope.striker[i] = true;
							}
                            numbering++;
                        } else if (monthStart &gt; i) {
                            $scope.calVal[i] = " ";
                            $scope.striker[i] = false;
                        }
                    } else {
                        if (numbering &lt;= $scope.data.maxMonthDays) {
                            $scope.calVal[i] = numbering;
							if($scope.reservedAndBlockedDates.indexOf(numbering)&gt;-1){
								$scope.striker[i] = true;
							}
                            numbering++;
                        } else {
                            $scope.calVal[i] = " ";
                            $scope.striker[i] = false;
                        }
                    }
					//check if the strike is needed on the date or not based on condition that its a date in the past
					if($scope.strikerValueYear == -1){
						$scope.striker[i] = true;
					}else if($scope.strikerValueMonth == -1 &amp;&amp; $scope.strikerValueYear == 0){
						$scope.striker[i] = true;
					}
                }


            });

        });

}]]&gt;&lt;/client_script&gt;&lt;controller_as&gt;c&lt;/controller_as&gt;&lt;css&gt;.center{
  text-align: center;
}

.notvalid{
  color: red;
}&lt;/css&gt;&lt;data_table&gt;sp_instance&lt;/data_table&gt;&lt;demo_data/&gt;&lt;description/&gt;&lt;docs/&gt;&lt;field_list/&gt;&lt;has_preview&gt;false&lt;/has_preview&gt;&lt;id&gt;comp231_calendar&lt;/id&gt;&lt;internal&gt;false&lt;/internal&gt;&lt;link&gt;&lt;![CDATA[function link(scope, element, attrs, controller) {
  
}]]&gt;&lt;/link&gt;&lt;name&gt;comp231_calendar&lt;/name&gt;&lt;option_schema/&gt;&lt;public&gt;false&lt;/public&gt;&lt;roles/&gt;&lt;script&gt;&lt;![CDATA[//Server Side Function
(function() {
    /* populate the 'data' object */
    /* e.g., data.table = $sp.getValue('table'); */
	
	//Notifying server to check the updated values send by client
    var value;
    if (input.date_and_time != '' &amp;&amp; input.date_and_time != null) {
        value = input.date_and_time;
    } else {
        value = new GlideDateTime();
    }

	//List of Variables being used for calculations
    data.maxMonthDays = 0;
    data.currentMonth = getMonth(value);
    data.currentMonthInNumber = new GlideDateTime(value).getMonth();
    data.currentYear = getYear(value);
    data.currentDay = getDay(value);
