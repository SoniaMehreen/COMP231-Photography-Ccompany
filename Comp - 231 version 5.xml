<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sys_script_include"&gt;&lt;sys_script_include action="INSERT_OR_UPDATE"&gt;&lt;access&gt;package_private&lt;/access&gt;&lt;active&gt;true&lt;/active&gt;&lt;api_name&gt;global.CheckSelectedDate&lt;/api_name&gt;&lt;caller_access/&gt;&lt;client_callable&gt;true&lt;/client_callable&gt;&lt;description/&gt;&lt;name&gt;CheckSelectedDate&lt;/name&gt;&lt;script&gt;&lt;![CDATA[var CheckSelectedDate = Class.create();
CheckSelectedDate.prototype = Object.extendsObject(AbstractAjaxProcessor, {

	//function to check if the date selected is already booked by another client
    checkBookings: function() {
        var bookingExists = false;

        var selected_date = this.getParameter('sysparm_date_time');
        var date = new GlideDateTime(selected_date).getDate();
        var y = date.getYear();
        var m = date.getMonth();
        m = ('0' + m).slice(-2);
        var d = date.getDayOfMonthUTC();
        d = ('0' + d).slice(-2);
        
        var strQry = "u_date_and_timeON" + y + "-" + m + "-" + d + "@javascript:gs.dateGenerate('" + y + "-" + m + "-" + d + "','start')@javascript:gs.dateGenerate('" + y + "-" + m + "-" + d + "','end')";

        var gr = new GlideRecord("u_event_bookings");
        gr.addEncodedQuery(strQry);
        gr.query();

        while (gr.next()) {
            bookingExists = true;
        }

        return bookingExists;
    },

	//Check if the selected date is in the past
    checkPastDate: function() {
        var ActualEndDate = this.getParameter('sysparm_end_date');
		ActualEndDate = new GlideDateTime(ActualEndDate).getDate();
        return parseInt(gs.dateDiff(gs.now(), ActualEndDate, true) / 86400);
    },

    type: 'CheckSelectedDate'
});]]&gt;&lt;/script&gt;&lt;sys_class_name&gt;sys_script_include&lt;/sys_class_name&gt;&lt;sys_created_by&gt;admin&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2020-04-11 08:37:21&lt;/sys_created_on&gt;&lt;sys_id&gt;f1b061a22f085c50d5b2d3f62799b695&lt;/sys_id&gt;&lt;sys_mod_count&gt;9&lt;/sys_mod_count&gt;&lt;sys_name&gt;CheckSelectedDate&lt;/sys_name&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sys_script_include_f1b061a22f085c50d5b2d3f62799b695&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;admin&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2020-04-11 10:07:45&lt;/sys_updated_on&gt;&lt;/sys_script_include&gt;&lt;/record_update&gt;</payload>
