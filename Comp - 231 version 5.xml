        }
        day = day - date + 1;
        return day;
    }

	//Check if the selected year is leap year or not
    function leapYear(year) {
        return ((year % 4 == 0) &amp;&amp; (year % 100 != 0)) || (year % 400 == 0);
    }

	//Get the day of the week for the selected date-time on the form
    function getDay(parm1) {
